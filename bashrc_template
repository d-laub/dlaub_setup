function pathprepend() {
    if [ -d "$1" ] && [[ ":$PATH:" != *":$1:"* ]]; then
	    PATH="$1${PATH:+":$PATH"}"
    fi
}

if [ -e $HOME/.bash_aliases ]; then
    source $HOME/.bash_aliases
fi

